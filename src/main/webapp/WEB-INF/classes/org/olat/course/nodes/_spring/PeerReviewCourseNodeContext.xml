<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
  http://www.springframework.org/schema/beans
  http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- Register Peer Review course node configuration outside the module JAR so it is auto-loaded -->
    <bean id="peerreview" class="org.olat.course.nodes.PeerReviewCourseNodeConfiguration" scope="prototype">
        <property name="order" value="115" />
    </bean>

</beans>
