<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
                        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- JMS Stuff for ActiveMQ -->
	<bean id="ltiConnectionFactory" class="org.apache.activemq.artemis.jms.client.ActiveMQJMSConnectionFactory" lazy-init="true">
		<property name="brokerURL" value="${lti.broker.url}" />
	</bean>

	<bean id="ltiQueue" class="org.apache.activemq.artemis.jms.client.ActiveMQQueue" lazy-init="true">
		<constructor-arg value="olat/ltiQueue" />
	</bean>
	
	
</beans>
